	pages = {446--449},
	annote = {Barnes-Hut Tree
},
	file = {Full Text PDF:C\:\\Users\\zebsu\\Zotero\\storage\\FALZ9GR6\\Barnes and Hut - 1986 - A hierarchical O(N log N) force-calculation algori.pdf:application/pdf},
