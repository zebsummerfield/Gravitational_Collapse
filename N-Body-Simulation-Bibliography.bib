@article{khoperskov_dynamical_2003,
	title = {A dynamical model of the {Galaxy}},
	volume = {47},
	issn = {1063-7729, 1562-6881},
	url = {http://link.springer.com/10.1134/1.1583771},
	doi = {10.1134/1.1583771},
	abstract = {A series of dynamical models of the Galaxy is constructed assuming that the entire disk is near the gravitational-stability limit. This imposes constraints on the dynamical and kinematic parameters of the main subsystems (the disk, bulge, and halo). The disk surface density in the solar neighborhood should not exceed 58 M / pc2. Further, we ﬁnd that the observed local decrease in the rotational velocity at 6 kpc r 10 kpc is not associated with details of the radial distribution of matter in the Galaxy and instead results from dynamical processes or some other factors responsible for noncircular motions. It follows from the presence of a long-lived bar and the observed distribution of the stellar-velocity dispersion that the central maximum in the rotation curve at radius r 300 pc cannot be associated with a very concentrated bulge core. The best agreement between the observational data and the parameters of the dynamical models is achieved for a radial disk scale length of L 3 kpc. The relative contribution of the disk to the circular rotational velocity at r = 2.2L is 73\%. c 2003 MAIK “Nauka/Interperiodica”.},
	language = {en},
